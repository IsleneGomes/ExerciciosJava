package pacoteDesafio;

import java.util.Scanner;

public class ClasseDesafio {

	public static void main(String[] args) {
	
			Scanner ent = new Scanner(System.in);
			int [] numeros = new int[10];
			System.out.println("Digite um valor para n: ");
			n = ent.nextInt();
			
			for (int i = 0; i < 10; i ++) {
				System.out.printf("Digite o %dº número:\n ", i + 1);
				numeros[i] = ent.nextInt();
			}
		System.out.printf("Valores entre 0 e 10: %d.\n Valores entre 11 e 20: %d\n Valores acima de 20: %d ", valores[0], valores[1], valores[2]);

	}

}
